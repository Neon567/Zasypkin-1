public class EvenNumbers {
    public static void main(String[] args) {
        // Начинаем цикл с числа 8 и продолжаем до 20 включительно
        for (int number = 8; number <= 20; number += 2) {
            // Выводим четное число в консоль
            System.out.println(number);
        }
    }
}
